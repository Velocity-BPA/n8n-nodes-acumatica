{
  "name": "n8n-nodes-acumatica",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Acumatica Cloud ERP providing 12 resources and 100+ operations for sales orders, customers, inventory, invoicing, purchasing, and financial operations through the Contract-Based REST API.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-acumatica.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-acumatica#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-acumatica/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "acumatica",
    "erp",
    "cloud-erp",
    "sales-orders",
    "inventory",
    "invoicing",
    "purchasing",
    "financial",
    "automation",
    "workflow",
    "integration"
  ],
  "main": "dist/nodes/Acumatica/Acumatica.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AcumaticaOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Acumatica/Acumatica.node.js",
      "dist/nodes/Acumatica/AcumaticaTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.22.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
